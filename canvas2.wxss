canvas {
  position: fixed;
  top: 0;
  left: 400px;
  width: 1125px;
  height: 1125px;
}

.bg{
  position: absolute;
   top:0;
   left:0;
   width:100%;
   height:100%;
   z-index:-1;
}

.tag {
  width: 200px;
  height: 200px;
  position: absolute;
  top: 30px;
  left: 25%;
  z-index: 3;
  border: 1px dashed #fff;
}

.preview {
  width: 100%;
  height: 100%;
  background: rgba(95, 94, 94, 0.9);
  position: absolute;
  z-index: 2;
}

.bottomAll {
  width: 100%;
  position: absolute;
  bottom: 5px;
  z-index: 3;
}
.btnAll{
  width: 100%;
  display: flex;
  flex-direction: row;
}
.item {
  width: 30%;
  margin-left: 6px;
  display: flex;
  margin-bottom: 20px;
  flex-direction: column;
}

.demo {
  width: 100%;
  height: 60px;
}

.btn {
  width: 100%;
  font-size: 14px;
}
